@model ProjeX.CQRS.Commands.DestinationCommands.CreateDestinationCommand

@{
    ViewData["Title"] = "NewDestination";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Yeni Tur Ekleme Sayfası(CQRS)</h1>

<form method="post">
    <label>Şehir</label>
    <input type="text" asp-for="City" class="form-control" />
    <br />
    <label>Gün-Gece Syısı</label>
    <input type="text" asp-for="DayNight" class="form-control" />
    <br />
    <label>Fiyat</label>
    <input type="text" asp-for="Price" class="form-control" />
    <br />
    <label>Kapasite</label>
    <input type="text" asp-for="Capacity" class="form-control" />
    <br />
    <button class="btn btn-outline-primary">KAYDET</button>
</form>

